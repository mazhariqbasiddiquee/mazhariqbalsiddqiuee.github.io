function openResume() {
  // Open a new window and load the PDF link
  window.open("https://drive.google.com/file/d/11hTC5RYegOuHZG7819Vy5TFMnjW6rAdY/view?usp=drive_link", "_blank");
  return false; // Prevent the default link behavior
}

<a id="resume-button-1" id="resume-link-1" href="https://drive.google.com/uc?export=download&id=11hTC5RYegOuHZG7819Vy5TFMnjW6rAdY" class="nav-link resume" target="_blank" onclick="openResume()">Resume</a>